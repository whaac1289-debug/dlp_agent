[Version]
Signature = "$Windows NT$"
Class = "File System"
ClassGuid = {71A27CDD-812A-11D0-BEC7-08002BE2092F}
Provider = %ManufacturerName%
CatalogFile = dlp_minifilter.cat
DriverVer = 01/01/2024,1.0.0.0

[DestinationDirs]
DefaultDestDir = 12
DlpMiniFilter.DriverFiles = 12

[DefaultInstall]
OptionDesc = %ServiceDesc%
CopyFiles = DlpMiniFilter.DriverFiles

[DefaultInstall.Services]
AddService = DlpMiniFilter,0x00000002,DlpMiniFilter.Service

[DlpMiniFilter.DriverFiles]
dlp_minifilter.sys

[DlpMiniFilter.Service]
DisplayName = %ServiceDesc%
ServiceBinary = %12%\dlp_minifilter.sys
ServiceType = 2
StartType = 3
ErrorControl = 1
LoadOrderGroup = "FSFilter Activity Monitor"
AddReg = DlpMiniFilter.AddReg

[DlpMiniFilter.AddReg]
HKR,,"DebugFlags",0x00010001,0x0
HKR,,"SupportedFeatures",0x00010001,0x3

[Strings]
ManufacturerName = "DLP Agent"
ServiceDesc = "DLP Minifilter"
